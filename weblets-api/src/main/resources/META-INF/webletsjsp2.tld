<taglib xmlns="http://java.sun.com/xml/ns/j2ee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd" version="2.0">

    <tlib-version>1.0</tlib-version>
    <jsp-version>2.0</jsp-version>
    <short-name>web</short-name>
    <uri>http://weblets.dev.java.net/tags_jsp2</uri>
   


    <function>
        <name>resource</name>
        <function-class>net.java.dev.weblets.WebletUtils</function-class>
        <function-signature>java.lang.String getResource( java.lang.String, java.lang.String)</function-signature>
    </function>

    <function>
          <name>url</name>
          <function-class>net.java.dev.weblets.WebletUtils</function-class>
          <function-signature>java.lang.String getURL( java.lang.String, java.lang.String)</function-signature>
    </function>

   
</taglib>
        

